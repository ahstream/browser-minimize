{
  "name": "browser-minimize",
  "description": "Browser Minimize Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "key": "",
  "permissions": ["storage", "activeTab", "tabs", "webNavigation"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_icon": {
      "16": "/images/icons/16.png",
      "32": "/images/icons/32.png",
      "48": "/images/icons/48.png",
      "128": "/images/icons/128.png"
    }
  },
  "icons": {
    "16": "/images/icons/16.png",
    "32": "/images/icons/32.png",
    "48": "/images/icons/48.png",
    "128": "/images/icons/128.png"
  },
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": []
}
